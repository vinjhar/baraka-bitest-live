{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["https://deno.land/x/deno@v1.40.5/mod.d.ts"]
  },
  "include": ["*.ts"],
  "exclude": ["node_modules"]
} 